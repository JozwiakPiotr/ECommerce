{
    "AllowedOrigins": "http://localhost:4200",
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "Browse": {
        "Product": {
            "PageSize": [ 5, 10, 15 ],
            "SearchPhrase": [ "name", "price" ]
        },
        "Order": {
            "PageSize": [ 5, 10, 15 ],
            "SearchPhrase": [ "email", "totalPrice" ]
        }
    },
    "Jwt": {
        "JwtKey": "PRIVATE_KEY_DONT_SHARE",
        "JwtExpireDays": 1,
        "JwtIssuer": "http://ECommerce.com"
    }
}